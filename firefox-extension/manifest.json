{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.3.4",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "<all_urls>",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "privacy",
    "downloads",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "config.js",
      "background/utils.js",
      "background/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/content.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "JS Eyes",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    "content/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "js-browser-extension@example.com",
      "strict_min_version": "58.0"
    }
  }
}
